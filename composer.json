{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/UMN-LATIS/laravel-shibboleth"
    },
        {
      "type": "git",
      "url": "https://github.com/UMN-LATIS/LTI-Tool-Provider-Library-PHP"
    }
    ],
    "require": {
        "php": ">=7.0.0",
        "deployer/deployer": "^6.3",
        "deployer/dist": "^6.3",
        "doctrine/dbal": "^2.7",
        "imsglobal/lti": "dev-master",
        "intervention/image": "^2.4",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "razorbacks/laravel-shibboleth": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "deployer/recipes": "^6.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
