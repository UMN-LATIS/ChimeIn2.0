    "test:watch": "npm run jest -- --watch",
    "test:e2e": "npm run cypress:headless",
    "test:unit": "npm run jest",
    "test": "npm run test:unit && npm run test:e2e",
    "lint": "eslint './**/*.{js,mjs,cjs,vue}'",
    "prettier:check": "prettier --check .",
